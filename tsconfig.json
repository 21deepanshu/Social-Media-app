{
  "extends": "./tsconfig.app.json",
  "compilerOptions": {
    "jsx": "react-jsx",
    "moduleResolution": "node",
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "strict": true,
    "target": "ESNext",
    "module": "ESNext",
    "resolveJsonModule": true,
    "baseUrl": "./src",
    "paths": {
      "@/*": ["./src/*"]
    }
  }
}
